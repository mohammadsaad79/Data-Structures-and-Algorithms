#include<bits/stdc++.h>
using namespace std;

int main() {
    int n, sum = 0;
    cout << "Enter any number: ";
    cin >> n;
    vector<int> divisors;
    for(int i = 1; i * i <= n; i++) {
        if(n % i == 0) {
            divisors.push_back(i);
            if(i != n/i)
                divisors.push_back(n/i);
        }
    }
    for(auto i : divisors)
        sum += i;
    cout << "Sum of divisors: " << sum << endl;
}
