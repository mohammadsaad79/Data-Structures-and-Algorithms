#include<bits/stdc++.h>
using namespace std;

int main() {
    int n, last_digit, count = 0;
    cout << "Enter any number: ";
    cin >> n;
    vector<int> digits(10, 0);
    while(n != 0) {
        last_digit = n % 10;
        digits[last_digit]++;
        n /= 10;
    }
    for(auto i : digits) {
        if(i > 1)
            count++;
    }
    cout << count << endl;
}
