#include<bits/stdc++.h>
using namespace std;

int main() {
    vector<int> arr = {1, 2, 4, 6};
    int max = *max_element(arr.begin(), arr.end());
    int lcm = max;
    int newlcm;
    for(int i = 0; i < arr.size(); i++) {
        if(max % arr[i] == 0) newlcm = max;
        else {
            max += lcm;
            i = -1;
        }
    }
    cout << newlcm << endl;
}
