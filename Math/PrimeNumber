#include<bits/stdc++.h>
using namespace std;

int main() {
    int n, count = 0;
    cout << "Enter any number: ";
    cin >> n;
    for(int i = 1; i * i <= n; i++) {
        if(n % i == 0) {
            count += 1;
            if(i != n/i)
                count += 1;
        }
    }
    if(count == 2)
        cout << n << " is prime";
    else
        cout << n << " is not prime";
}
