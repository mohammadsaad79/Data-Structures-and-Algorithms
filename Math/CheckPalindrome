#include<bits/stdc++.h>
using namespace std;

int main() {
    int n, last_digit, rev = 0;
    cout << "Enter any number: ";
    cin >> n;
    int num = n;
    while(n != 0) {
        last_digit = n % 10;
        rev = rev * 10 + last_digit;
        n /= 10;
    }
    if(rev == num)
        cout << rev << " is palindrome";
    else
        cout << rev << " is not palindrome";
}
