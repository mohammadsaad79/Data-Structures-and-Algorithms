void arrayToDLL(int n, int arr[]) {
    if(n == 0) return;
    Node* head = new Node(arr[0]);
    Node* curr = head;
    for(int i = 1; i < n; i++) {
        Node* temp = new Node(arr[i]);
        temp -> prev = curr;
        curr -> next = temp;
        curr = temp;
    }
    while(head != NULL) {
        cout << head -> data << " ";
        head = head -> next;
    }
}
