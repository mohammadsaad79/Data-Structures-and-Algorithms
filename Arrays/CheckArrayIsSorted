#include<bits/stdc++.h>
using namespace std;

bool isArraySorted(int arr[], int n) {
    for(int i = 0; i < n - 1; i++)
        if(arr[i] > arr[i + 1]) return false;
    return true;
}

int main() {
    int n = 6;
    int arr[n] = {1, 2, 4, 3, 5, 6};
    bool result = isArraySorted(arr, n);
    if(result == 1) cout << "true";
    else cout << "false";
    return 0;
}
